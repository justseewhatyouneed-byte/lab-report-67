#include <stdio.h>

int main() {
    int n, i, j = 0;

    printf("Enter number of elements (sorted array): ");
    scanf("%d", &n);

    int arr[n];

    printf("Enter %d elements in sorted order:\n", n);
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    // Removing duplicates in-place
    int temp[n];  // temporary array to store unique elements
    temp[0] = arr[0];

    for(i = 1; i < n; i++) {
        if(arr[i] != arr[i - 1]) {
            j++;
            temp[j] = arr[i];
        }
    }

    printf("\nArray after removing duplicates:\n");
    for(i = 0; i <= j; i++) {
        printf("%d ", temp[i]);
    }
    printf("\n");

    return 0;
}
