#include <stdio.h>

int main() {
    int m, n, i, j, temp;

    printf("Enter number of rows: ");
    scanf("%d", &m);

    printf("Enter number of columns: ");
    scanf("%d", &n);

    int matrix[m][n];

    // Input matrix elements
    printf("Enter elements of the matrix:\n");
    for (i = 0; i < m; i++) {
        for (j = 0; j < n; j++) {
            scanf("%d", &matrix[i][j]);
        }
    }

    // Interchange first and last element of each column
    for (j = 0; j < n; j++) {
        temp = matrix[0][j];
        matrix[0][j] = matrix[m - 1][j];
        matrix[m - 1][j] = temp;
    }

    // Print updated matrix
    printf("Matrix after interchanging first and last elements of each column:\n");
    for (i = 0; i < m; i++) {
        for (j = 0; j < n; j++) {
            printf("%d ", matrix[i][j]);
        }
        printf("\n");
    }

    return 0;
}
