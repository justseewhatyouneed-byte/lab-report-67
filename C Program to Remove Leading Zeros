#include <stdio.h>
#include <string.h>

int main() {
    char str[100];
    int i = 0;

    printf("Enter a numeric string: ");
    scanf("%s", str);

    // Skip all leading zeros
    while (str[i] == '0') {
        i++;
    }

    // If string contains only zeros
    if (str[i] == '\0') {
        printf("0\n");
    } else {
        printf("String without leading zeros: %s\n", &str[i]);
    }

    return 0;
}
