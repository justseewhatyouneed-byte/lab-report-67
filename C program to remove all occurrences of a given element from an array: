#include <stdio.h>

int main() {
    int n, i, key, k = 0;

    printf("Enter number of elements: ");
    scanf("%d", &n);

    int arr[n];

    printf("Enter %d elements:\n", n);
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    printf("Enter the element to remove: ");
    scanf("%d", &key);

    // Remove occurrences of key
    int newArr[n]; // temporary array
    for(i = 0; i < n; i++) {
        if(arr[i] != key) {
            newArr[k] = arr[i];
            k++;
        }
    }

    // Print array after removal
    printf("\nArray after removing %d:\n", key);
    if(k == 0) {
        printf("All elements removed. Array is empty.\n");
    } else {
        for(i = 0; i < k; i++) {
            printf("%d ", newArr[i]);
        }
        printf("\n");
    }

    return 0;
}
