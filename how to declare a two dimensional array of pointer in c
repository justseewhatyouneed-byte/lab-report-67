#include <stdio.h>

int main() {
    int *arr[3][4];  // 2D array of pointers to int (3 rows, 4 columns)

    // Example: Allocate memory for each pointer
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 4; j++) {
            arr[i][j] = NULL;  // Can also point to some int variable
        }
    }

    return 0;
}
