#include <stdio.h>

int main() {
    int a[3][3];
    int i, j;
    int det;

    // Input matrix
    printf("Enter the elements of a 3x3 matrix:\n");
    for (i = 0; i < 3; i++) {
        for (j = 0; j < 3; j++) {
            scanf("%d", &a[i][j]);
        }
    }

    // Calculate determinant
    det =  a[0][0] * (a[1][1]*a[2][2] - a[1][2]*a[2][1])
         - a[0][1] * (a[1][0]*a[2][2] - a[1][2]*a[2][0])
         + a[0][2] * (a[1][0]*a[2][1] - a[1][1]*a[2][0]);

    // Print result
    printf("Determinant of the matrix = %d\n", det);

    return 0;
}
